CREATE DATABASE DongHo CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci;
CREATE TABLE SanPham(
                      MaSanPham	INT PRIMARY KEY NOT NULL AUTO_INCREMENT,
                      Ten VARCHAR(200),
                      Danhmuc VARCHAR(50),
                      Day VARCHAR(50),
                      Loai VARCHAR(50),
                      Gia BIGINT,
                      Mota TEXT,
                      Anh1 VARCHAR(150),
                      Anh2 VARCHAR(150),
                      Anh3 VARCHAR(150),
                      Anh4 VARCHAR(150),
                      Hang VARCHAR(50),
                      Xuatxu VARCHAR(50)
);

